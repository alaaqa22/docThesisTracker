<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="4.0" xmlns="https://jakarta.ee/xml/ns/jakartaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
                                  https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_4_0.xsd">
    <navigation-rule>
        <from-view-id>/views/anonymous/login.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{loginBacking.login}</from-action>
            <from-outcome>/view/authenticated/circulationslist?faces-redirect=true</from-outcome>
            <to-view-id>/views/authenticated/circulationslist.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

</faces-config>